{
  "languageserver": {
    "go": {
      "command": "gopls",
      "rootPatterns": ["go.mod"],
      "trace.server": "verbose",
      "filetypes": ["go"]
    },
    "terraform": {
      "command": "terraform-ls",
      "args": ["serve"],
      "filetypes": ["terraform", "tf"],
      "initializationOptions": {}
    }
  },
  "python.linting.banditEnabled": true,
  "python.linting.mypyEnabled": true,
  "git.addGBlameToVirtualText": true
}
